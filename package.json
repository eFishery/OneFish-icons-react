{
  "name": "@onefish/icons-react",
  "version": "0.1.1",
  "license": "MIT",
  "author": "eFishery's engineer <engineer@efishery.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "optimize-svg": "svgo -f './assets/efishery'",
    "collate": "node bin/collate.js",
    "assemble": "node bin/assemble.js",
    "pre-commit": "yarn lint --fix && yarn test --onlyChanged"
  },
  "module": "dist/icons-react.esm.js",
  "repository": "https://github.com/eFishery/OneFish-icons-react",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "efishery",
    "onefish",
    "design system",
    "icons",
    "svg",
    "react"
  ],
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "chalk": "^4.1.1",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "svgo": "^2.3.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "auto"
  }
}
